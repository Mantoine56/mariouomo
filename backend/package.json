{
  "name": "mario-uomo-backend",
  "version": "0.1.0",
  "description": "Mario Uomo E-commerce Platform Backend",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:explicit": "ts-node -r tsconfig-paths/register src/main.ts",
    "debug:server": "ts-node -r tsconfig-paths/register src/debug.ts",
    "debug:server:no-db": "ts-node -r tsconfig-paths/register src/debug-no-db.ts",
    "debug:server:with-db": "ts-node -r tsconfig-paths/register src/debug-with-db.ts",
    "debug:bootstrap": "ts-node -r tsconfig-paths/register src/debug-bootstrap.ts",
    "test:db-connection": "ts-node -r tsconfig-paths/register src/debug-db-test.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "jest --config ./test/jest-integration.json",
    "test:integration:db": "NODE_ENV=test jest --config ./test/jest-integration.json --runInBand --forceExit",
    "test:integration:db:products": "NODE_ENV=test jest --config ./test/jest-integration.json --runInBand --forceExit --testPathPattern=products",
    "test:integration:db:analytics": "NODE_ENV=test jest --config ./test/jest-integration.json --runInBand --forceExit --testPathPattern=analytics",
    "seed": "nest start --entryFile dist/seed/seed-command"
  },
  "dependencies": {
    "@keystonejs/keystone": "^19.3.4",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^10.0.0",
    "@prisma/client": "^5.2.0",
    "@sentry/node": "^7.64.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "aws-sdk": "^2.1692.0",
    "cache-manager": "^6.4.0",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.5.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",
    "nanoid": "3.3.7",
    "nest-commander": "^3.16.1",
    "nestjs-typeorm-paginate": "^4.0.4",
    "newrelic": "^12.14.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "typeorm": "^0.3.20",
    "uuid": "^11.1.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.0",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^4.10.4",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/newrelic": "^9.14.7",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.9",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.2.0",
    "socket.io": "^4.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": "18.x"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
